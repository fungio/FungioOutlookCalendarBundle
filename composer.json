{
    "name": "fungio/outlook-calendar-bundle",
    "type": "symfony-bundle",
    "description": "Provides a outlook calendar integration for your Symfony 3 Project.",
    "keywords": [
        "outlook calendar"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pierrick AUBIN",
            "email": "pierrick.aubin@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "symfony/framework-bundle": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "2.8|~3.0",
        "symfony/yaml": "~2.0",
        "twig/twig": "~1.12"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "Fungio\\OutlookCalendarBundle\\": ""
        }
    },
    "target-dir": "Fungio/OutlookCalendarBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "3.x.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
